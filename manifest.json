{
  "name": "boss直聘插件",
  "version": "1.0",
  "description": "boss直聘一键勾搭",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs",
    "https://www.zhipin.com/"
  ],
  "page_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/icon3.png",
      "32": "images/icon3.png",
      "48": "images/icon3.png",
      "128": "images/icon3.png"
    }
  },
  "icons": {
    "16": "images/icon3.png",
    "32": "images/icon3.png",
    "48": "images/icon3.png",
    "128": "images/icon3.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  }
}
